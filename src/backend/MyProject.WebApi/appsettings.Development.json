{
  "Serilog": {
    "Using": ["Serilog.Sinks.Seq"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:20008"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "Database": "Host=localhost;Port=20004;Database=recipes;Username=admin;Password=pwd"
  },
  "Caching": {
    "DefaultExpiration": "00:10:00",
    "Redis": {
      "Enabled": true,
      "ConnectionString": "localhost:20006",
      "Password": "redis-dev-password",
      "UseSsl": false,
      "DefaultDatabase": 0,
      "InstanceName": "recipes:",
      "ConnectTimeoutMs": 5000,
      "SyncTimeoutMs": 5000,
      "AsyncTimeoutMs": 5000,
      "AbortOnConnectFail": false,
      "ConnectRetry": 3,
      "KeepAliveSeconds": 60
    },
    "InMemory": {
      "SizeLimit": 1024,
      "ExpirationScanFrequency": "00:01:00",
      "CompactionPercentage": 0.05
    }
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    }
  },
  "Cors": {
    "AllowAllOrigins": true,
    "AllowedOrigins": [],
    "PolicyName": "DefaultCorsPolicy"
  }
}
